{
  "name": "rats-pixi",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prestart": "run-s clean",
    "start": "vite --config devtools/config.dev.mjs",
    "clean": "rimraf dist/* public/assets/* .assetpack/*",
    "prebuild": "run-s clean format:check lint types",
    "build": "vite build --config devtools/config.build.mjs",
    "prepreview": "run-s build",
    "preview": "vite preview --config devtools/config.build.mjs --open",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test-assetpack": "assetpack"
  },
  "devDependencies": {
    "@assetpack/core": "1.7.0",
    "@types/sharp": "0.31.1",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "eslint": "9.39.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "npm-run-all": "4.1.5",
    "prettier": "3.6.2",
    "rimraf": "6.1.2",
    "sharp": "0.34.5",
    "typescript": "5.9.3",
    "vite": "7.2.2",
    "xml-js": "^1.6.11",
    "zod": "^4.1.13"
  },
  "dependencies": {
    "@pixi/devtools": "^2.0.1",
    "@pixi/layout": "3.2.0",
    "@pixi/sound": "6.0.1",
    "@pixi/tilemap": "5.0.2",
    "@pixi/ui": "^2.3.2",
    "@tweakpane/core": "^2.0.5",
    "animejs": "4.2.2",
    "phaser-box2d": "github:phaserjs/phaser-box2d",
    "pixi-filters": "^6.1.4",
    "pixi.js": "8.14.2",
    "pixijs-input-devices": "^0.7.4",
    "tweakpane": "^4.0.5"
  }
}
